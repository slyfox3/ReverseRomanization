{
  "manifest_version": 3,
  "name": "Reverse Romanization of Names",
  "version": "1.0",
  "description": "Show names in the text of original language.",
  "content_scripts": [
    {
      "matches": ["https://digitalpool.com/*"],
      "js": [
        "name_mapping.js",
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
